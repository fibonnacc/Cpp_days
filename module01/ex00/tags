$(NAME)	Makefile	/^$(NAME): $(OBJ)$/;"	t
%.o	Makefile	/^%.o: %.cpp$/;"	t
CC	Makefile	/^CC = c++$/;"	m
NAME	Makefile	/^NAME = cpp$/;"	m
OBJ	Makefile	/^OBJ = $(SRC:.cpp=.o)$/;"	m
SRC	Makefile	/^SRC = main.cpp	Zombie.cpp	newZombie.cpp	randomChump.cpp$/;"	m
ZOMBIE_HPP	Zombie.hpp	/^#define ZOMBIE_HPP$/;"	d
Zombie	Zombie.hpp	/^class Zombie$/;"	c
all	Makefile	/^all: $(NAME)$/;"	t
announce	Zombie.cpp	/^void  Zombie::announce(void)$/;"	f	class:Zombie	typeref:typename:void
clean	Makefile	/^clean:$/;"	t
fclean	Makefile	/^fclean: clean$/;"	t
main	main.cpp	/^int main ()$/;"	f	typeref:typename:int
name	Zombie.hpp	/^    std::string name;$/;"	m	class:Zombie	typeref:typename:std::string
newZombie	newZombie.cpp	/^Zombie* newZombie( std::string name )$/;"	f	typeref:typename:Zombie *
randomChump	randomChump.cpp	/^void Zombie::randomChump( std::string name )$/;"	f	class:Zombie	typeref:typename:void
re	Makefile	/^re: fclean all$/;"	t
~Zombie	Zombie.hpp	/^  ~Zombie()$/;"	f	class:Zombie
